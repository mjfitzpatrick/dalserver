package cds.savot.pull;

/**
 * <p>
 * Statistics
 * </p>
 * * @author Andre Schaaff
 * 
 * @version 3.0 (kickoff 31 May 02)
 */
public class SavotStatistics {

    int iTablesGlobal = 0; // index of TABLE element
    int iTablesLocal = 0; // index of TABLE element in current RESOURCE
    long iTRGlobal = 0L; // index of TR element
    long iTRLocal = 0L; // index of TR element in current TABLE
    long iTDGlobal = 0L; // index of TD element
    long iTDLocal = 0L; // index of TD element in current TABLE
    int iResources = 0; // index of RESOURCES element
    long iGroupsGlobal = 0L; // index of GROUP element

    public void iTablesGlobalInc() {
	iTablesGlobal++;
    }

    public void iTablesGlobalDec() {
	iTablesGlobal--;
    }

    public void iTablesGlobalReset() {
	iTablesGlobal = 0;
    }

    public void iTablesGlobalSet(int value) {
	iTablesGlobal = value;
    }

    public int iTablesGlobalGet() {
	return iTablesGlobal;
    }

    public void iTablesLocalInc() {
	iTablesLocal++;
    }

    public void iTablesLocalDec() {
	iTablesLocal--;
    }

    public void iTablesLocalReset() {
	iTablesLocal = 0;
    }

    public void iTablesLocalSet(int value) {
	iTablesLocal = value;
    }

    public int iTablesLocalGet() {
	return iTablesLocal;
    }

    public void iTRGlobalInc() {
	iTRGlobal++;
    }

    public void iTRGlobalDec() {
	iTRGlobal--;
    }

    public void iTRGlobalReset() {
	iTRGlobal = 0L;
    }

    public void iTRGlobalSet(long value) {
	iTRGlobal = value;
    }

    public long iTRGlobalGet() {
	return iTRGlobal;
    }

    public void iTRLocalInc() {
	iTRLocal++;
    }

    public void iTRLocalDec() {
	iTRLocal--;
    }

    public void iTRLocalReset() {
	iTRLocal = 0L;
    }

    public void iTRLocalSet(long value) {
	iTRLocal = value;
    }

    public long iTRLocalGet() {
	return iTRLocal;
    }

    public void iTDGlobalInc() {
	iTDGlobal++;
    }

    public void iTDGlobalDec() {
	iTDGlobal--;
    }

    public void iTDGlobalReset() {
	iTDGlobal = 0L;
    }

    public void iTDGlobalSet(long value) {
	iTDGlobal = value;
    }

    public long iTDGlobalGet() {
	return iTDGlobal;
    }

    public void iTDLocalInc() {
	iTDLocal++;
    }

    public void iTDLocalDec() {
	iTDLocal--;
    }

    public void iTDLocalReset() {
	iTDLocal = 0L;
    }

    public void iTDLocalSet(long value) {
	iTDLocal = value;
    }

    public long iTDLocalGet() {
	return iTDLocal;
    }

    public void iResourcesInc() {
	iResources++;
    }

    public void iResourcesDec() {
	iResources--;
    }

    public void iResourcesReset() {
	iResources = 0;
    }

    public void iResources(int value) {
	iResources = value;
    }

    public long iResourcesGet() {
	return iResources;
    }

    public void iGroupsGlobalInc() {
	iGroupsGlobal++;
    }

    public void iGroupsGlobalDec() {
	iGroupsGlobal--;
    }

    public void iGroupsGlobalReset() {
	iGroupsGlobal = 0;
    }

    public void iGroupsGlobal(long value) {
	iGroupsGlobal = value;
    }

    public long iGroupsGlobalGet() {
	return iGroupsGlobal;
    }

}
